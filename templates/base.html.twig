<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
 
        
        {% block stylesheets %}
                <!-- Add site Favicon -->
                <link rel="shortcut icon" href="{{ asset('/main/assets/images/favicon/favicon.ico')}} " type="image/png">


                <!-- vendor css (Icon Font) -->
                <link rel="stylesheet" href="{{ asset('/main/assets/css/vendor/bootstrap.bundle.min.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/vendor/pe-icon-7-stroke.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/vendor/font.awesome.css')}} " />

                <!-- plugins css (All Plugins Files) -->
                <link rel="stylesheet" href="{{ asset('/main/assets/css/plugins/animate.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/plugins/swiper-bundle.min.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/plugins/jquery-ui.min.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/plugins/nice-select.css')}} " />
                <link rel="stylesheet" href="{{ asset('/main/assets/css/plugins/venobox.css')}} " /> 

                <!-- Main Style -->
                <link rel="stylesheet" href="{{ asset('/main/assets/css/style.css')}} " /> 

        {% endblock %}




    </head>
    <body>
        {% block body %}{% endblock %}  

        
        {% block javascripts %}
                 <!-- Vendor JS -->
            <script src="{{ asset('/main/assets/js/vendor/jquery-3.6.0.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/vendor/bootstrap.bundle.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/vendor/jquery-migrate-3.3.2.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/vendor/modernizr-3.11.2.min.js')}}"></script>

            <!--Plugins JS-->
            <script src="{{ asset('/main/assets/js/plugins/swiper-bundle.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/jquery-ui.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/jquery.nice-select.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/countdown.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/scrollup.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/jquery.zoom.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/venobox.min.js')}}"></script>
            <script src="{{ asset('/main/assets/js/plugins/ajax-mail.js')}}"></script>

 

            <!-- Main Js -->
            <script src="{{ asset('/main/assets/js/main.js')}}"></script>
            
        {% endblock %}


    </body>
</html>
